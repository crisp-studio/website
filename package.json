{
  "name": "dekema-website",
  "description": "Dekema Website",
  "version": "1.0.0",
  "author": "Leo Bernard <leo@crisp.studio>",
  "browserslist": "cover 99.5%",
  "contributors": [
    "Daniel Wirtz <daniel@crisp.studio>"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv-safe": "^6.0.0",
    "gatsby": "^2.0.0",
    "gatsby-plugin-catch-links": "^2.0.3",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-google-tagmanager": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-typescript": "^2.0.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "gql-codegen": "apollo codegen:generate --queries=src/**/*.tsx --schema=http://localhost:8000/___graphql --tagName=graphql --target=typescript --outputFlat src/types",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix-all": "tslint --fix -p . && prettier --write src/**/*",
    "fix": "lint-staged"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/googlemaps": "^3.30.15",
    "@types/lodash.isequal": "^4.5.3",
    "@types/react-helmet": "^5.0.7",
    "apollo": "^1.9.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write"
    ],
    "**/*.{css,scss,sass,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crisp-studio/website"
  }
}
